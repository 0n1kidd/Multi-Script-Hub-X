section8:NewButton("Night", "", function()

    while true do 

		print ("Loop cycle start")

		game.Lighting.TimeOfDay = "18:00:00"

		wait(0.5)

	end

end)

section8:NewButton("Disco", "", function()

    while true do

 

		game.Lighting.Ambient = Color3.new(math.random(), math.random(), math.random())

		wait(.25)

 

	end

end)
