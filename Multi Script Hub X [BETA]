-- // Booting Library \\ --

local library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

-- // Functions \\ --

local colors = {

    SchemeColor = Color3.fromRGB(88, 88, 88),

    Background = Color3.fromRGB(88, 88, 88),

    Header = Color3.fromRGB(0, 0, 0),

    TextColor = Color3.fromRGB(255, 255, 255),

    ElementColor = Color3.fromRGB(0, 0, 0),

}

-- // Creating Window \\ --

local window = library.CreateLib("Multi Script Hub X [BETA]", colors)

-- // Define Tabs \\ --

